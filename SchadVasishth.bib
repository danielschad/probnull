	Issue = {1},
	Journal = {Cognitive Science},
	Pages = {5--50},
	Pdf = {http://www.ling.uni-potsdam.de/~vasishth/pdfs/PatilEtAlCogSci2014.pdf},
	Title = {{A computational evaluation of sentence comprehension deficits in aphasia}},
	Volume = {40},
	Year = {2016},
	Bdsk-Url-1 = {https://doi.org/10.1111/cogs.12250}}

